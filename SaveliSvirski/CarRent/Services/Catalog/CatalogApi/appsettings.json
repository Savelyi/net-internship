{
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Warning"
      }
    }
  },
  "ElasticConfiguration": {
    "Uri": "http://localhost:9200"
  },
  "AuthConfig": {
    "Authority": "http://localhost:3410",
    "Audience": "CatalogApi"
  },
  "MassTransit": {
    "Url": "localhost",
    "Host": "/",
    "CatalogQueue": "catalog-listener",
    "RentQueue": "rent-listener",
    "SignalRQueue": "signalr-listener",
    "UserName": "user",
    "UserPas": "userpas"
  },
  "ConnectionStrings": {
    "DefaultConnection": "Host=localhost;Port=5432;Database=CatalogDb;User Id=user;Password=user;"
  },
  "Redis": {
    "InstanceName": "CatalogCache",
    "RedisHost": "localhost",
    "RedisPort": "6379"
  },
  "SignalRConfig": {
    "DefaultConnection": "http://localhost:3000"
  },
  "AllowedHosts": "*"
}
